# RationSmart PWA Environment Variables
# Copy this to .env and fill in the values
# Note: .env.development and .env.production are auto-loaded by Vite

# API Configuration (REQUIRED)
# Development: http://localhost:8000
# Production: Your Railway backend URL (e.g., https://your-app.up.railway.app)
VITE_API_BASE_URL=http://localhost:8000

# API Key (optional - for authenticated admin endpoints)
VITE_APP_API_KEY=

# Environment marker (development | production)
VITE_APP_ENV=development

# Push Notifications - VAPID Keys (optional)
# Generate with: npx web-push generate-vapid-keys
VITE_VAPID_PUBLIC_KEY=
